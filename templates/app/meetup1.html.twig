            let CHANNEL = '{{ channel.channel }}';
            let DEFAULT_CHANNEL = 'chatapp_'+CHANNEL;


            let connection = new RTCMultiConnection();


            //const io = require('js/app/socket.io');


            function doreset()
            {
                counter = 59;
                countDownInterval = null;
                updateInterval = null;
                $( "#videos-container" ).empty();
                $( "#videos-second-container" ).empty();
            }

            function start_countdown()
            {
                console.log("start_countdown() called");
                _interval = setInterval(function(){
                    console.log(counter);
                    counter--
                    $('.span_counter_label').html(counter);
                    if(counter <=5){
                        $('.videoCounterWrap').addClass('videoCounterWrapActive');
                    }
                    if (counter <= 0)
                    {
                        $('.videoCounterWrap').removeClass('videoCounterWrapActive');
                        $('#partnerWrap').hide();
                        $('#partnerLoading').show();
                        $('#titlePage').text('Waiting for the next meeting.');
                        $('#partnerWrap').removeClass('active');
                        connection.socket.disconnect();
                        console.log("connection.socket.disconnect(): called");
                        clearInterval(_interval);
                        endChat();
                        $('#chatRoom').show();
                        //location.reload();
                    }
                }, 1000);
            }

            calculateVideoSize();
            $(document).resize(function(){
                calculateVideoSize();
            });

            function create_UUID(){
                let dt = new Date().getTime();
                let uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
                    let r = (dt + Math.random()*16)%16 | 0;
                    dt = Math.floor(dt/16);
                    return (c=='x' ? r :(r&0x3|0x8)).toString(16);
                });
                return uuid;
            }


            connect();
            function connect()
            {
                updateInterval = setInterval(function(){updateChannel()},2000);

                //let DEFAULT_CHANNEL = 'chatapp_'+channel;
                console.log(DEFAULT_CHANNEL);
                connection.socketURL = 'https://chatapp.mobi:2087/';
                connection.enableFileSharing = false; // by default, it is "false".
                connection.userid = create_UUID();
                connection.session = {
                    audio: true,
                    video: true,
                };
                connection.sdpConstraints.mandatory = {
                    OfferToReceiveAudio: true,
                    OfferToReceiveVideo: true
                };
                connection.onstream = function (event) {
                    if (event.type === 'local') {
                        document.getElementById('videos-container').appendChild(event.mediaElement);
                    }

                    if (event.type === 'remote' && remote_joiners === 0) {

                        document.getElementById('videos-second-container').appendChild(event.mediaElement);
                        start_countdown();
                        remote_joiners++;
                    }
                };
                connection.onstreamended = function (event) {
                    console.log('onstreamended');
                    console.log(event);
                    clearInterval(_interval);
                    counter = 59;
                    $('.span_counter_label').html(counter);
                }
                connection.openOrJoin(DEFAULT_CHANNEL);
                $("#chatRoom").hide();
            }

            function doSearch()
            {
                console.log("doSearch() called");
                doreset();
                backToWait();
                doMeetPeople();
            }

            $( "#dosearch" ).click(function() {
                doSearch();
            });

            function updateChannel(){
                $.ajax({
                    url: '{{ path('update_chat_facetime') }}?hash={{ hash }}&room='+CHANNEL,
                    type: 'POST',
                    dataType: 'json',
                }).done(function(response) {
                    setTimeout(updateChannel(),2000);
                    if(response.ok){
                        if(response.partner){
                            $('#partnerWrap').addClass('active');
                            $('#partner_name').text(response.partner.name);
                            $('#partner_gender').text(response.partner.gender);
                            $('#partner_avatar').css("background-image", "url(" + response.partner.avatar + ")");
                            $('#btnRegister').data("partner", response.partner.hash);
                            $('#partnerWrap').show();
                            $('#partnerLoading').hide();
                            $('#titlePage').text('Greetings!');
                        }
                    }else{
                        {#document.location='{{ path('facetime_register') }}';#}
                    }

                });
            }

            function backToWait(){
                $('#chatRoom').hide();
                $('#countdownWrap').text(count);
                $('#countDown').show();
                $('#countdownWrap').addClass('countdownWrapActive');

                countDownInterval = setInterval(function () {
                    count = count-1;
                    $('#countdownWrap').text(count);
                    if(count == 0){
                        clearInterval(countDownInterval);
                        count = 5;
                        $('#countdownWrap').html('<table align="center" style="margin-top:90px;"><tr><td><div style="line-height: 30px;"><img src="/static/images/chatload.gif"/></div></td></tr><tr><td style="font-size: 16px; line-height: 25px;">Please wait a moment.</td></tr></table>');
                    }
                },1000)
            }

            function endChat() {
                console.log("endChat(): called");
                $.ajax({
                    url: '{{ path('end_chat') }}?room='+CHANNEL,
                    type: 'POST',
                    dataType: 'json',
                }).done(function(response) {
                    console.log(response);
                });
            }

            function doMeetPeople() {
                $.ajax({
                    url: '{{ path('facetime_getchannel') }}?hash={{ hash }}',
                    type: 'POST',
                    dataType: 'json',
                }).done(function(response) {
                    console.log(response);
                    if(response.channel){
                        //let chat_url = url+response.channel.channel;
                        //document.location = chat_url;
                        CHANNEL = response.channel.channel;
                        connect();
                        //updateChannel();
                        $('#countdownWrap').hide();
                    }

                });
            }

            function calculateVideoSize(){
                let w = parseFloat($('#big-container').width());
                let h = 480*w/600;
                $('#big-container').css({'height':h+'px'});
            }
        });


        function backToWait(){
            $('#chatRoom').hide();
            $('#countdownWrap').text(count);
            $('#countDown').show();
            $('#countdownWrap').addClass('countdownWrapActive');

            countDownInterval = setInterval(function () {
